  "devDependencies": {
    "@angular/tsc-wrapped": "^4.4.7",
    "@ionic/app-scripts": "3.1.9",
    "@types/bluebird": "^3.5.21",
    "@types/chai": "^4.1.4",
    "@types/faker": "^4.1.2",
