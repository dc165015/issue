{
  "name": "bn2",
  "version": "0.0.1",
  "author": "dc",
  "homepage": "http://booknebula.com/",
  "private": true,
  "scripts": {
    "api": "cd api && export ROOT_URL=http://192.168.2.3:3000 && meteor run --settings private/settings.json --inspect",
    "api:reset": "export RESET_DB=1 && npm run serve",
    "api:debug": "cd api && export ROOT_URL=http://192.168.2.3:3000 && meteor run --inspect-brk --port 9229",
    "bookstore": "cd bookstore && meteor run --port 8085",
    "bookstore:debug": "cd bookstore && meteor run --inspect-brk --port 9229",
    "dc:test": "cd api/server/lib/docschema-checker && clear && mocha -r ./node_modules/ts-node/register test.ts",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "meteor-client:bundle": "meteor-client bundle -s api -c meteor-client.config.json",
    "serve": "npm run meteor-client:bundle && npm run api",
    "debug": "concurrently  \"./.run\" \"npm run api:debug | tee output.log\"",
    "reset": "concurrently  \"./.run\" \"npm run api:reset | tee output.log\""
  },
  "mocha.requires": [
    "ts-node/register"
  ],
  "dependencies": {
    "@agm/core": "^1.0.0-beta.3",
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@babel/runtime": "^7.0.0-beta.55",
    "@ionic-native/app-rate": "^4.9.0",
    "@ionic-native/barcode-scanner": "^4.10.0",
    "@ionic-native/camera": "^4.9.0",
    "@ionic-native/core": "4.7.0",
    "@ionic-native/crop": "^4.9.0",
    "@ionic-native/geolocation": "^4.8.0",
    "@ionic-native/image-picker": "^4.8.0",
    "@ionic-native/sim": "^4.9.0",
    "@ionic-native/splash-screen": "^4.7.0",
    "@ionic-native/status-bar": "^4.7.0",
    "@ionic/storage": "^2.1.3",
    "angular2-moment": "^1.9.0",
    "autoprefixer": "^8.6.5",
    "babel-runtime": "^6.26.0",
