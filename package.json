      "cordova-plugin-sim": {},
      "cordova-plugin-sms-receiver": {},
      "cordova-plugin-camera": {},
      "cordova-plugin-crop": {},
      "cordova-plugin-smsreceiver": {},
      "phonegap-plugin-barcodescanner": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      }
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
